platform :osx, "10.10"
workspace 'Gureum'

target 'OSX' do
   project 'Gureum.xcodeproj'
   pod 'cdebug'
   pod 'FoundationExtension', '>= 1.2.4'
   pod 'Fabric'
   pod 'Crashlytics'
end
target 'OSXTests' do
   project 'Gureum.xcodeproj'
   pod 'cdebug'
   pod 'FoundationExtension', '>= 1.2.4'
end

target :'App' do
    platform :ios, "9.0"
    project 'iOS.xcodeproj'
    pod 'FoundationExtension/UIKitExtension'
    pod 'Fabric'
    pod 'Crashlytics'
end
target :'iOS' do
    platform :ios, "9.0"
    project 'iOS.xcodeproj'
    pod 'FoundationExtension/UIKitExtension'
    pod 'Fabric'
    pod 'Crashlytics'
end
